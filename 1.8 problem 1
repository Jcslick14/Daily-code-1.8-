#while loop to print the numbers 0-10 counting by 2s!
num = 0 #starting point
while num<50: #go one above 10 so it prints
    print(num, end = " ")
    num += 5 #what you count by
    
#while loop to print "Broncos" until the user types "Ps2"
UserInput = "somthing" #dummy value
while UserInput != "javier stinks":
    UserInput = input("Broncos")
